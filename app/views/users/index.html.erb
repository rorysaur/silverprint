<h1>all users</h1>

<table class="table">
  <% @users.try(:each) do |user| %>
    <tr>
      <td>
        <%= image_tag user.profile_pic.url(:thumbnail) %>
        <%= link_to user.username, user_url(user) %>
        <%= "(following)" if current_user.is_following?(user) %>
      </td>
    </tr>
  <% end %>
</table>