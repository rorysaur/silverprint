<td>
  <%= view.imageTag(user.get('profilePicUrl')) %>
  <%= view.linkTo(user.get('username'), "#users/" + user.id) %>
  <% if (user.isFollowedBy(Silverprint.currentUser)) { %>
    <button class="unfollow btn btn-default btn-sm">unfollow</button>
  <% } else if (user.id != Silverprint.currentUser.id) { %>
    <button class="follow btn btn-default btn-sm">follow</button>
  <% } %>
</td>
